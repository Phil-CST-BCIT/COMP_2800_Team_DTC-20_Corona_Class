<!-- result page for quiz -->
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" type="text/css" href="css/index.css" />
    <link rel='stylesheet' type='text/css' href='css/quizScore.css'/>
    <link
      href="https://fonts.googleapis.com/css2?family=Oxygen:wght@300;400;700&display=swap"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.12.1/css/all.min.css"
    />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/velocity/2.0.5/velocity.min.js"></script>

    <title>CoronaClass</title>
  </head>
  <body>
    <div class="wrapper">
      <div class='btn-box top-box'>
          <div class='btn' id='home'>
              <a href='/index'><button>Home</button></a>
          </div>
      </div>

      <div id='scoreInfo'>
        <div class ='info' id='title'>
            <h1>RESULTS</h1>
        </div>
        <div class='info score-detail'>
            <h2>You correctly answered <span id='score'>#</span> questions out of 5!</h2>
        </div>
      </div>

      <div id='questionSummary'>
        <!-- insert user answer/correct answer for each question -->
      </div>

      <div class='btn-box bottom-box'>
        <h2 id='tryAgain'>Try Again?</h2>
        <div class='btn' id='retrybtn'>
          <a href='/quizStart'>
            <button>Retry</button>
          </a>
        </div>

      </div>

    </div>

    <script src="/js/index.js"></script>
    <script src=""></script>
  </body>
</html>
